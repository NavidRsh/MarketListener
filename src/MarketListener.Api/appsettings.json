{
  "ConnectionStrings": {
    "BaseConnection": "Server=.;Database=MarketListener;Integrated Security=True;MultipleActiveResultSets=true;TrustServerCertificate=True;Connect Timeout=1000;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Issuer": "MarketListenerApi",
    "Audience": "ApiClient",
    "DurationInMinutes": 600,
    "Key": "SecretKey"
  },
  "AllowedHosts": "*",
  "Sieve": {
    "CaseSensitive": "false",
    "DefaultPageSize": "10",
    "MaxPageSize": "10000",
    "ThrowExceptions": "true"
  }
}
